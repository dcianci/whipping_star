<?xml version="1.0" ?>

<mode name="nu" use="1"/>
<detector name="uBooNE" use="1" />

<channel name="elike" unit="MeV" use="1" numbins="4">
		<bins
			edges="100 500 900 1300 1800"
		/>
		<subchannel name="intrinsic" use="1" osc = "11"/>
		<subchannel name="cocktail" use="1" osc = "22"/>
		<subchannel name="extbnb" use="1" osc="0"/>
		<subchannel name="fullosc" use="1" osc="21"/>
</channel>

<!-- ############################# MultiSims #####################################################-->
<MultisimFile treename="nue_intrinsic_tree" filename="/uboone/data/users/yatesla/othersys/inputs_to_sbnfit/arxiv/input_to_sbnfit_August2018.root" scale = ".286" maxevents ="20000" >
	<parameters names = "ALL"/>
	<branch
		name="reco_energy"
		type="double"
		associated_subchannel="nu_uBooNE_elike_intrinsic"
		oscillate = "true"
		true_param_name = "true_nu_energy"
		true_L_name = "true_nu_L"/>
</MultisimFile>

